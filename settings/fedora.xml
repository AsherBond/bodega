<ivysettings>
    <settings defaultResolver="default">
        <resolvers>
            <chain name="default" returnFirst="true">
                <filesystem name="eucalyptus">
                    <artifact pattern="/usr/share/eucalyptus/[artifact]-[revision].[ext]"/>
                </filesystem>
                <filesystem name="fedora-1">
                    <artifact pattern="/usr/share/java/[artifact]-[revision].[ext]"/>
                </filesystem>
                <filesystem name="fedora-2">
                    <artifact pattern="/usr/share/java/[artifact].[ext]"/>
                </filesystem>
            </chain>
        </resolvers>
    </settings>
</ivysettings>

